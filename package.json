{
  "name": "react-playground",
  "version": "1.0.0",
  "description": "just a playground for learning react",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && webpack",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "dev": "webpack-dev-server",
    "lint": "eslint . --ext .js --ext .jsx; exit 0",
    "lint:precommit": "eslint . --ext .js --ext .jsx;",
    "lint:watch": "watch 'npm run lint' ./",
    "test": "mocha --compilers js:babel-core/register --require ./test/test-helper.js 'test/**/.@(js|jsx)'",
    "test:watch": "npm test -- --watch",
    "start": "babel-node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenny-f/react-playground.git"
  },
  "author": "Kenny F",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kenny-f/react-playground/issues"
  },
  "homepage": "https://github.com/kenny-f/react-playground#readme",
  "devDependencies": {
    "babel-cli": "6.4.0",
    "babel-core": "6.4.0",
    "babel-eslint": "4.1.6",
    "babel-loader": "6.2.1",
    "babel-plugin-react-transform": "2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "chai": "3.4.1",
    "chai-immutable": "1.5.3",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "3.1.0",
    "eslint-plugin-babel": "3.0.0",
    "eslint-plugin-react": "3.15.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "jsdom": "7.2.2",
    "mocha": "2.3.4",
    "react-transform-catch-errors": "1.0.1",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.2.0",
    "redux-logger": "2.3.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "watch": "0.17.1",
    "webpack": "1.12.11",
    "webpack-dev-middleware": "1.4.0",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.6.0"
  },
  "dependencies": {
    "bootstrap": "3.3.6",
    "dotenv": "1.2.0",
    "express": "4.13.3",
    "history": "1.17.0",
    "immutable": "3.7.6",
    "jquery": "^2.1.4",
    "radium": "^0.15.3",
    "react": "0.14.6",
    "react-bootstrap": "0.28.2",
    "react-bootstrap-grid": "1.0.0",
    "react-dom": "0.14.6",
    "react-redux": "4.0.6",
    "react-router": "1.0.3",
    "redux": "3.0.5",
    "redux-thunk": "1.0.3"
  },
  "pre-commit": []
}
