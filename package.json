{
  "name": "react-playground",
  "version": "1.0.0",
  "description": "just a playground for learning react",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "coverage": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- \"./src/**/*.spec*\"",
    "build": "npm run clean && webpack",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "dev": "webpack-dev-server",
    "lint": "eslint . --ext .js --ext .jsx; exit 0",
    "lint:precommit": "eslint . --ext .js --ext .jsx;",
    "lint:watch": "watch 'npm run lint' ./",
    "test": "mocha --compilers js:babel-core/register './src/**/*.spec*'",
    "test:watch": "npm test -- --watch",
    "start": "npm run start-client& npm run start-server",
    "start-client": "babel-node .",
    "start-server": "nodemon --exec babel-node ./server --watch server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenny-f/react-playground.git"
  },
  "author": "Kenny F",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kenny-f/react-playground/issues"
  },
  "homepage": "https://github.com/kenny-f/react-playground#readme",
  "devDependencies": {
    "assert": "1.3.0",
    "babel-cli": "6.4.0",
    "babel-core": "6.4.0",
    "babel-eslint": "4.1.6",
    "babel-istanbul": "0.6.0",
    "babel-loader": "6.2.1",
    "babel-plugin-react-transform": "2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "3.1.0",
    "eslint-plugin-babel": "3.0.0",
    "eslint-plugin-react": "3.15.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "mocha": "2.3.4",
    "nodemon": "1.8.1",
    "react-transform-catch-errors": "1.0.1",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.2.0",
    "redux-logger": "2.3.2",
    "should": "8.1.1",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "watch": "0.17.1",
    "webpack": "1.12.11",
    "webpack-dev-middleware": "1.4.0",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.6.0"
  },
  "dependencies": {
    "body-parser": "1.14.2",
    "cors": "2.7.1",
    "dotenv": "1.2.0",
    "express": "4.13.3",
    "history": "1.17.0",
    "isomorphic-fetch": "2.2.1",
    "joi": "7.2.2",
    "jscheck": "0.2.0",
    "koa": "1.1.2",
    "material-ui": "0.14.2",
    "radium": "^0.15.3",
    "react": "0.14.6",
    "react-bootstrap-grid": "1.0.0",
    "react-dom": "0.14.6",
    "react-redux": "4.0.6",
    "react-router": "2.0.0-rc5",
    "react-router-redux": "2.1.0",
    "react-tap-event-plugin": "0.2.1",
    "redux": "3.0.5",
    "redux-form": "4.1.6",
    "redux-saga": "0.4.1",
    "seamless-immutable": "5.0.1"
  },
  "pre-commit": []
}
