{
  "name": "react-playground",
  "version": "1.0.0",
  "description": "just a playground for learning react",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "dev": "webpack-dev-server",
    "lint": "eslint . --ext .js --ext .jsx; exit 0",
    "test": "mocha --compilers js:babel-core/register --require ./test/test-helper.js 'test/**/.@(js|jsx)'",
    "test:watch": "npm test -- --watch",
    "start": "babel-node server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenny-f/react-playground.git"
  },
  "author": "Kenny F",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kenny-f/react-playground/issues"
  },
  "homepage": "https://github.com/kenny-f/react-playground#readme",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "html-webpack-plugin": "^1.7.0",
    "jsdom": "^7.2.0",
    "mocha": "^2.3.4",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "dotenv": "^1.2.0",
    "immutable": "^3.7.5",
    "react": "^0.14.3",
    "react-bootstrap": "^0.28.1",
    "react-bootstrap-grid": "^1.0.0",
    "react-dom": "^0.14.3"
  }
}
